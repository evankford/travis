{
  "name": "travis",
  "title": "Pokshin Theme",
  "description": "A custom wp theme for Pokshin.",
  "version": "0.1.0",
  "author": {
    "name": "evankford",
    "email": "evankerrickford@gmail.com",
    "url": "http://evankerrickford.com"
  },
  "scripts": {
    "build": "wpackio-scripts build",
    "dev": "webpack --watch --config webpack.config.js",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/_s.pot' --domain '_s'",
    "bootstrap": "wpackio-scripts bootstrap",
    "build-backup": "webpack --mode=production --config webpack.config.js",
    "start": "wpackio-scripts start",
    "archive": "wpackio-scripts pack"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@wpackio/scripts": "^3.5.0",
    "EventEmitter": "^1.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "img-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.1",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "three": "^0.125.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.1.0",
    "webpack-glsl-loader": "^1.0.1",
    "wp-pot-cli": "^1.2.1"
  },
  "keywords": [],
  "dependencies": {
    "@wpackio/entrypoint": "^3.5.0",
    "glsl-blend": "^1.0.3",
    "glslify": "^7.0.0",
    "gyronorm": "^2.0.6",
    "lazysizes": "^5.1.1",
    "postprocessing": "^6.6.0",
    "three-orbitcontrols": "^2.102.2",
    "underscore": "^1.9.1"
  }
}
